public class Money {
    private int dollars;
    private int cents;

    
    public Money(int dollars, int cents) {
        this.dollars = dollars;
        this.cents = cents;
    }

    
    public Money(Money other) {
        this.dollars = other.dollars;
        this.cents = other.cents;
    }

    

    public int getDollars() {
        return dollars;
    }

    public int getCents() {
        return cents;
    }

    
}
